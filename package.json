{
  "name": "next-map",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit",
    "deploy:vercel": "vercel --prod",
    "docker:dev": "docker compose -f docker/docker-compose.dev.yml up --build",
    "docker:prod": "docker compose -f docker/docker-compose.yml up --build",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:up:build": "docker compose -f docker/docker-compose.yml up --build -d",
    "docker:up:dev": "docker compose -f docker/docker-compose.dev.yml up -d",
    "docker:up:dev:build": "docker compose -f docker/docker-compose.dev.yml up --build -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:down:dev": "docker compose -f docker/docker-compose.dev.yml down",
    "docker:down:volumes": "docker compose -f docker/docker-compose.yml down -v",
    "docker:down:dev:volumes": "docker compose -f docker/docker-compose.dev.yml down -v",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "docker:logs:dev": "docker compose -f docker/docker-compose.dev.yml logs -f",
    "docker:logs:app": "docker compose -f docker/docker-compose.yml logs -f app",
    "docker:logs:app:dev": "docker compose -f docker/docker-compose.dev.yml logs -f app-dev",
    "docker:logs:postgres": "docker compose -f docker/docker-compose.yml logs -f postgres",
    "docker:shell": "docker compose -f docker/docker-compose.yml exec app sh",
    "docker:shell:dev": "docker compose -f docker/docker-compose.dev.yml exec app-dev sh",
    "docker:db:shell": "docker compose -f docker/docker-compose.yml exec postgres psql -U nextmap -d nextmap",
    "docker:db:shell:dev": "docker compose -f docker/docker-compose.dev.yml exec postgres psql -U nextmap -d nextmap",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:reset": "docker compose -f docker/docker-compose.yml down -v && docker system prune -f && docker volume prune -f",
    "docker:reset:dev": "docker compose -f docker/docker-compose.dev.yml down -v && docker system prune -f && docker volume prune -f"
  },
  "packageManager": "pnpm@9.12.3",
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@floating-ui/react": "^0.27.16",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "i18next": "^25.6.0",
    "maplibre-gl": "^5.9.0",
    "next": "15.5.6",
    "next-auth": "^4.24.10",
    "next-intl": "^4.3.12",
    "prisma": "^5.22.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-map-gl": "^8.1.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.11.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}
